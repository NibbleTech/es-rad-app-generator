{
    "name": "theshaunwalker/es-rad-skeleton-generator",
    "type": "library",
    "autoload": {
        "psr-4": {
            "EsRadAppGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EsRadAppGenerator\\TestHelpers\\": "tests/Helpers/"
        }
    },
    "authors": [
        {
            "name": "Shaun Walker",
            "email": "shaunandrewwalker@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nette/php-generator": "^3.6",
        "ramsey/uuid": "^4.2",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "vimeo/psalm": "^4.18",
        "rector/rector": "^0.12.12",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "roave/infection-static-analysis-plugin": "^1.16"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true
        }
    }
}
