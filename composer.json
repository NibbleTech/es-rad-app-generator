{
    "name": "nibbletech/es-rad-app-generator",
    "type": "library",
    "autoload": {
        "psr-4": {
            "NibbleTech\\EsRadAppGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NibbleTech\\EsRadAppGenerator\\TestHelpers\\": "tests/Helpers/"
        }
    },
    "authors": [
        {
            "name": "Shaun Walker",
            "email": "shaunandrewwalker@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nette/php-generator": "^3.6",
        "ramsey/uuid": "^4.3",
        "ext-simplexml": "*",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "vimeo/psalm": "^4.23",
        "rector/rector": "^0.13.7",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "roave/infection-static-analysis-plugin": "^1.20",
        "psalm/plugin-phpunit": "^0.17.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true
        }
    }
}
